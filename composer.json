{
    "name": "nina/nina",
    "description": "A Symfony CMS - Content Management System / Système de gestion de contenu de site web sous Symfony",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "Nina",
        "CMS",
        "Symfony"
    ],
    "homepage": "https://www.cms-nina.fr",
    "authors": [
        {
            "name": "Nina Community - la couleur du Zèbre",
            "email": "contact@cms-nina.fr",
            "homepage": "https://www.cms-nina.fr"
        }
    ],
    "support": {
        "email": "contact@cms-nina.fr"
    },
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "easycorp/easyadmin-bundle": "^2.3",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/user-bundle": "dev-master",
        "mpdf/mpdf": "^7.1",
        "php-translation/symfony-bundle": "^0.8.1",
        "psr/simple-cache": "^1.0",
        "scssphp/scssphp": "^1.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Theme\\": "themes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@php install.php",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C69V525Z6GW3M3F0WPAEA0AA",
            "allow-contrib": false
        }
    }
}
